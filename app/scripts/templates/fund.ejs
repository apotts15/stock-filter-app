<div class="bg-image">
    <div class="container fund">
        <div class="row header-row" style="height: 220px">
            <div class="col s12 center-align">
                <div class="row">
                    <!-- JST data.fund.fund -->
                    <h3 class="fund-name center-align"><%= fund.fund %></h3>
                </div>
                <div class="row">
                    <h5 class="btn center-align fund-ticker"><span class="bold"><%= ticker %></span></h5>
                </div>
            </div>
        </div>

        <div class="card gunstocks overview">
            <div class="card-content">
                <!-- JST data.fund.categoryData -->
                <h5 class="bold"><%= fund.gunStocks && fund.gunStocks.length === 1 ? fund.gunStocks.length + ' Gun Stock' : fund.gunStocks.length + ' Gun Stocks' %></h5>
                <div class="row">

                    <div class="col s6 m6 l3">
                        <div class="card hoverable <%= fund.sinStocks.total && fund.sinStocks.total.metal ? fund.sinStocks.total.metal : 'missing-metal' %>" data-id="<%= fund.ticker %>">
                            <div class="card-bg">
                                <h6 class="card-title ticker btn white"><%= fund.ticker %></h6>
                                <div class="metals">
                                    <img class="metal" src="/images/metals/<%= fund.sinStocks.total && fund.sinStocks.total.metal ? fund.sinStocks.total.metal : 'none' %>.png" alt="<%= fund.sinStocks.total && fund.sinStocks.total.metal ? fund.sinStocks.total.metal : 'No Metal' %>" class="<%= fund.sinStocks.total && fund.sinStocks.total.metal ? fund.sinStocks.total.metal : 'no-metal' %>" />
                                </div>
                                <!--<h5 class="fund-name"><%= fund.fund %></h5>-->
                                <p class="fund-type center-align">
                                <%= fund.fundBasics.segment %>
                                </p>
                            </div>
                            <div class="card-content center">
                                <h2 class="card-percentage center-align">
                                    <span class="bold"><%= fund.sinStocks.total && fund.sinStocks.total.allocationPercent ? fund.sinStocks.total.allocationPercent : 'missing-allocation' %></span>
                                </h2>
                                <h5 class="card-percentage-description">in stocks against your values</h5>
                            </div>
                        </div>

                        <!--<div class="card" data-id="<%= fund.ticker %>">-->
