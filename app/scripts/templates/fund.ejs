                    <i class="material-icons right-bottom control_point">control_point</i>
                </div>
            </div>
            <div class="carousel-item">
                <div class="card hoverable none" data-id="DRIP">
                    <div class="card-header center">none
                        <i class="activator material-icons right-top more_vert">more_vert</i>
                    </div>
                    <div class="card-summary-container">
                        <h3 class="card-title ticker white-text">DRIP</h3>
                        <p class="fund-name">Direxion Daily S&amp;P Oil &amp; Gas Exp. &amp; Prod. Bear 3X Shares</p>
                        <div class="fund-summary">
                            <div class="col s6">
                                <h4 class="value">-30.99%</h4>
                                <p class="label caps">Return</p>
                            </div>
                            <div class="vert-divider"></div>
                            <div class="col s6">
                                <h4 class="value">5.67%</h4>
                                <p class="label caps">Risk</p>
                            </div>
                        </div>
                    </div>
                    <div class="card-reveal medal-summary">
                        <div class="card-header center">none
                            <span class="card-title">
                                    <i class="close material-icons right-top more_vert">more_vert</i>
                                </span>
                        </div>

                        <div class="card-bg">
                            <h6 class="fund-type caps">Inverse Equity: U.S. Oil &amp; Gas Exploration &amp; Production</h6>
                            <div class="metals">
                                <img class="metal" src="/images/metals/none.png" alt="none">
