<h1>Funds from Company</h1>
<p>These ETFs all contain position from
    <company>
</p>

<div class="row">
    <div class="col-xs-12">
        <ul id="doctorUl">
            <% _.each(funds, function(fund, i) { %>
            <li data-id="<%= fund._id %>">
                <div class="row">
                    <div class="col-xs-2">
                        <img src="<%= doc.image %>" width="160" height="90"/>
                    </div>
                    <div class="col-xs-10">
                        <h3><%= fund.name %></h3>
                        <p> &#xf100; <%= fund.email %></p>
                    </div>
                </div>
            </li>
            <% }) %>
        </ul>
    </div>
</div>
