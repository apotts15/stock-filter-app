<div class="col s12 m4">
    <h4 class="onepebble-secondary-text">SDG Summary</h4>
    <div class="card">
        <div class="card-content sdg-header sdg-<%= sdg.goalNumber %>">
            <img class="sdg-icon" width="42px" src="/images/SDGs/SDG-<%= sdg.goalNumber %>-white.png" alt="<%= sdg.name %>" />
            <h2 class="sdg-goal">Goal <%= sdg.goalNumber %>:</h2>
            <h3 class="sdg-name"><%= sdg.name %></h3>
        </div>
        <div class="card-content">
            <p class="sdg-description"><%= sdg.description %></p>
        </div>
    </div>
</div>

