<div class="cards-container">
    <div class="row">
        <nav class="subpage col s12">
            <div class="nav-wrapper">

            </div>
        </nav>
        <div class="col s12 cards-col"> <!-- Note that "m8 l9" was added -->

            <div class="col s12 cards-list-header">
                <!-- JST data.funds.length -->
                <h6><%= funds.length %> Funds Found</h6>
            </div>
            <!--<div class="col s6 right-align">
                <ul class="pagination tiddy-bottom">
                    <li class="disabled"><a href="#!"><i class="material-icons">chevron_left</i></a></li>
                    <li class="active"><a href="#!">1</a></li>
                    <li class="waves-effect"><a href="#!">2</a></li>
                    <li class="waves-effect"><a href="#!">3</a></li>
                    <li class="waves-effect"><a href="#!">4</a></li>
                    <li class="waves-effect"><a href="#!">5</a></li>
                    <li class="waves-effect"><a href="#!"><i class="material-icons">chevron_right</i></a></li>
                </ul>
            </div>-->
            <ul class="card-list">
                <!-- JST data.funds -->
                <% _.each(funds, function(fund, i) { %>
                <li class="card-wrapper">
                    <div class="card hoverable" data-id="<%= fund.ticker %>">
                        <div class="card-bg background-<%= Math.floor(Math.random() * 6) + 1 %>">
                            <h6 class="card-title ticker btn blue darken-1"><%= fund.ticker %></h6>
                            <h5 class="fund-name"><%= fund.fund %></h5>
                            <p class="fund-type center-align">
                               <%= fund.fundBasics.segment %>
                            </p>
                        </div>
                        <div class="card-content center <%= fund.sinStocks.guns.hasSinStocks ? 'gun-full' : 'gun-free' %>">
                            <% if (fund.sinStocks.guns.hasSinStocks) { %>
                                <i class="material-icons clear left">clear</i><h5>Has Gun Stock</h5>
                            <% } else { %>
                                <i class="material-icons check left">check</i><h5>Gun-free Stock</h5>
                            <% } %>
                            <!--<p><i class="material-icons trending_up">trending_up</i><%= fund.analysis.fundClosureRisk %></p>-->
                                <i class="material-icons timeline left">timeline</i><h5><%= fund.analysis.fundClosureRisk %> Risk</h5>

                        </div>
                    </div>
                </li>
                <% }) %>
            </ul>
        </div>
    </div>
