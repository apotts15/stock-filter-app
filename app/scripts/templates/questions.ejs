<button class="btn waves-effect waves-light stock-filter-secondary exit">Exit</button>
<button class="btn waves-effect waves-light stock-filter-secondary index">Index</button>
<div id="questionnaire" class="container carousel carousel-slider" data-indicators="true">
    <% _.each(questions, function(question) { %>
        <div class="carousel-item" href="#<%= question.number %>!" id="question-<%= question.numeric_num %>">
            <!--<div class="row">-->
                <!--<div class="col s12">-->
                    <!--<div class="progress">-->
                        <!--<div class="determinate" style="width: <%= 100*(question.numeric_num/questions.length) %>%"></div>-->
                    <!--</div>-->
                <!--</div>-->
            <!--</div>-->
            <div class="row">
                <div class="col s12 m6 l3 progress-title">
                    <p>Question <%= question.numeric_num %>/<%= questions.length %></p>
                </div>
                <div class="col s12 m6 l3"></div>
                <div class="col s12 m6 l3"></div>
                <div class="col s12 m6 l3"></div>
            </div><!-- Page Content goes here -->
            <div class="row">
                <div class="col s12 m4 l2">
                </div>
                <div class="col s12 m4 l8">
                    <div class="center-align">
                        <i class="material-icons category-icons" style="background: url('images/icons/icons8-<%= question.icon %>-Filled-50.png')"/>
                    </div>
                    <h4><%= question.question %></h4>

                    <form action="#" class="questionnaire-answers">
                        <% _.each(question.answers, function(answer, i) { %>
                        <p>
                            <input class="with-gap" name="question-<%= question.numeric_num %>" type="radio" id="question-<%= question.numeric_num %>-answer-<%= i %>" value="<%= answer %>-<%= question.category %>" data-id="<%= question.category %>"/>
                            <label for="question-<%= question.numeric_num %>-answer-<%= i %>"><%= answer %></label>
                        </p>
                        <% }) %>
                    </form>
                </div>
                <div class="col s12 m4 l2"></div>
            </div>
            <div class="row">
                <div class="col m6 left-align">
                    <a class="waves-effect waves-light btn stock-filter-primary previous<%= question.numeric_num === 1 ? ' disabled' : '' %>">Previous</a>
                </div>
                <div class="col s12 m6 right-align<%= question.numeric_num !== questions.length ? '' : ' submit-container' %>">
                    <% if (question.numeric_num !== questions.length) { %>
                        <a class="waves-effect waves-light btn stock-filter-primary next" data-id="indeterminate-<%= question.category %>">Next</a>
                    <% } else { %>
                        <a class="waves-effect waves-light btn stock-filter-highlight submit" data-id="indeterminate-<%= question.category %>">Submit</a>
                    <% } %>
                </div>
            </div><!-- Page Content goes here -->
        </div>
    <% }) %>
</div>